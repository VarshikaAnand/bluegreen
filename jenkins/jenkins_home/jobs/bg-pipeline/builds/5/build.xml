    <script>pipeline {
    agent any

    environment {
        DOCKERHUB = credentials(&apos;dockerhub&apos;)
        IMAGE = &quot;varshika05/bluegreen-app:${BUILD_NUMBER}&quot;
    }

    stages {
        stage(&apos;Clone Repo&apos;) {
            steps {
                git branch: &apos;main&apos;,
                    url: &apos;https://github.com/VarshikaAnand/bluegreen.git&apos;
            }
        }

        stage(&apos;Build Docker Image&apos;) {
            steps {
                sh &quot;docker build -t ${IMAGE} .&quot;
            }
        }

        stage(&apos;Push Image&apos;) {
            steps {
                sh &quot;echo ${DOCKERHUB_PSW} | docker login -u ${DOCKERHUB_USR} --password-stdin&quot;
                sh &quot;docker push ${IMAGE}&quot;
            }
        }

        stage(&apos;Blue-Green Deploy&apos;) {
            steps {
                sh &quot;bash scripts/deploy_blue_green.sh ${IMAGE}&quot;
            }
        }
    }
}
</script>
